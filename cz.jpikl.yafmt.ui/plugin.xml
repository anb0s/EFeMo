<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="cz.jpikl.yafmt.ui.ModelMarker"
         name="Model Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
      <attribute
            name="cz.jpikl.yafmt.ui.ModelMarker.ObjectsURI">
      </attribute>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="cz.jpikl.yafmt.ui.perspectives.FeatureModelingPerspectiveFactory"
            icon="icons/fm-perspective.png"
            id="cz.jpikl.yafmt.ui.perspectives.FeatureModelingPerspective"
            name="Feature Modeling">
      </perspective>
   </extension>

</plugin>
