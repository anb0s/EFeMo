<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="cz.jpikl.yafmt.ui.editors.fm.FeatureModelEditor"
            contributorClass="cz.jpikl.yafmt.ui.editors.fm.FeatureModelEditorContributor"
            extensions="yafm"
            icon="icons/fm-file.png"
            id="cz.jpikl.yafmt.ui.editors.fm"
            name="Feature Model Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="cz.jpikl.yafmt.ui.wizards"
            name="YAFMT">
      </category>
      <wizard
            category="cz.jpikl.yafmt.ui.wizards"
            class="cz.jpikl.yafmt.ui.editors.fm.wizards.NewFeatureModelWizard"
            icon="icons/fm-file-new.png"
            id="cz.jpikl.yafmt.ui.editors.fm.wizards.NewFeatureModelWizard"
            name="New Feature Model">
      </wizard>
   </extension>

</plugin>
