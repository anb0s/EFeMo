<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="cz.jpikl.yafmt.ui.editors.fc.FeatureConfigurationEditor"
            contributorClass="cz.jpikl.yafmt.ui.editors.fc.FeatureConfigurationEditorContributor"
            extensions="yafc"
            icon="icons/fc-file.png"
            id="cz.jpikl.yafmt.ui.editors.fc"
            name="Feature Configuration Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="cz.jpikl.yafmt.ui.wizards"
            name="YAFMT">
      </category>
      <wizard
            category="cz.jpikl.yafmt.ui.wizards"
            class="cz.jpikl.yafmt.ui.editors.fc.wizards.NewFeatureConfigurationWizard"
            icon="icons/fc-file-new.png"
            id="cz.jpikl.yafmt.ui.editors.fc.wizards.NewFeatureConfigurationWizard"
            name="New Feature Configuration">
      </wizard>
   </extension>

</plugin>
